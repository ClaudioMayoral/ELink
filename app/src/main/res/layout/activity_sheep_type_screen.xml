<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SheepTypeScreen">

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="24dp"
        android:layout_height="22dp"
        app:srcCompat="@drawable/abc_vector_test"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="26dp" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sheepy,"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="138dp" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="70dp"
        android:layout_height="70dp"
        tools:layout_editor_absoluteX="177dp"
        tools:layout_editor_absoluteY="26dp"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="339dp"
        android:layout_height="39dp"
        android:text="Haremos que el crear y mejorar al mundo, sea una experiencia única. Cuéntanos de tu proyecto."
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="191dp" />

    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="340dp"
        android:layout_height="44dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Nombre del proyecto"
        tools:layout_editor_absoluteX="47dp"
        tools:layout_editor_absoluteY="262dp" />

    <EditText
        android:id="@+id/editTextTextPersonName3"
        android:layout_width="340dp"
        android:layout_height="44dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Meta monetaria (en USD)"
        tools:layout_editor_absoluteX="47dp"
        tools:layout_editor_absoluteY="378dp" />

    <EditText
        android:id="@+id/editTextTextPersonName4"
        android:layout_width="340dp"
        android:layout_height="44dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Objetivo del proyecto"
        tools:layout_editor_absoluteX="47dp"
        tools:layout_editor_absoluteY="320dp" />

    <Spinner
        android:id="@+id/spinner"
        android:layout_width="334dp"
        android:layout_height="39dp"
        tools:layout_editor_absoluteX="47dp"
        tools:layout_editor_absoluteY="435dp" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Zona de impacto"
        tools:layout_editor_absoluteX="50dp"
        tools:layout_editor_absoluteY="491dp" />

    <Button
        android:id="@+id/button12"
        android:layout_width="248dp"
        android:layout_height="54dp"
        android:text="Terminar registro"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="651dp" />

    <com.google.android.gms.maps.MapView
        android:id="@+id/mapView"
        android:layout_width="222dp"
        android:layout_height="114dp"
        tools:layout_editor_absoluteX="99dp"
        tools:layout_editor_absoluteY="528dp" />

</androidx.constraintlayout.widget.ConstraintLayout>